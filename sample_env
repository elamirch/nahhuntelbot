# This is a sample .env file
# Any value other than True for USE_PROXY will disable proxy settings in curl

# API keys
BOT_TOKEN= YOUR_TELEGRAM_BOT_TOKEN_GOES_HERE
MODEL_PROVIDER_API_KEY= YOUR_API_KEY_GOES_HERE

# Proxy setting
USE_PROXY=True

# Database settings
DB_HOST=localhost
